import java.util.*;
public class Main
{
    public static boolean isPrime(int n) 
    { 
             //Both 0 and 1 are not prime numbers. Prime numbers must be greater than 1 with exactly two divisors.
        if(n<=1)
        {
            return false;
        }

        // Check divisibility from 2 to n-1
        for(int i=2;i<n;i++)
        // Check divisibility from 2 to the square root of n
        for (int i = 2; i* i <=n; i++)
        {
            if(n%i==0)
            {
                return false;
            }
        }
        return true;
    }
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		if(isPrime(n))
		{
		    System.out.print("prime no.");
		}
		else{
		    System.out.print("not prime no.");  
		}

	}
}
