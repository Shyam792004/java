import java.util.*;
class Main
{
    public static long missingNumber(int []arr,int n)
    {
        
        long total_sum=n*(n+1)/2;
        long actual_sum=0;
        for(int i=0;i<n-1;i++)
        {
            actual_sum+=arr[i];
        }
        return total_sum-actual_sum;
        
    }
    
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n-1;i++)
        {
            arr[i]=sc.nextInt();
        }
        
        long ans=missingNumber(arr,n);
        System.out.println(ans);
        
        
    }
}
