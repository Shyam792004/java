import java.util.*;
public class Main
{
     public static int nthFib(int n) {
        if(n == 0) return 0;  // Edge case
        if(n == 1) return 1;  // Edge case

        int a = 0, b = 1, c = 0;
        for(int i = 2; i <= n; i++) {
            c = a + b;
            a = b;
            b = c;
        }
        return c;
    }

	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int result=nthfib(n);
		System.out.println(result);

	}
}
