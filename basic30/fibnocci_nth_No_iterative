import java.util.*;
public class Main
{
    public static int nthfib(int n) 
    { 
        int a=0;
        int b=1;
        int c=0;
        for(int i=2;i<=n;i++)
        {
            c=a+b;
            a=b;
            b=c;
        }
        return c;
        
        
    }
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int result=nthfib(n);
		System.out.println(result);

	}
}
