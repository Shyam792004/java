import java.util.*;
class Main
{
    
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        String s=sc.nextLine();
        
        StringBuilder sb=new StringBuilder();
        Stack<Character>st=new Stack<>();
        
        for(int i=0;i<s.length();i++)
        {
            if(s.charAt(i)!=' ')
            {
                st.push(s.charAt(i));
            }
            else{
                while(!st.isEmpty())
                {
                    sb.append(st.pop());
                }
                sb.append(" ");
            }
        }
        while(!st.isEmpty())
        {
            sb.append(st.pop());
        }
        System.out.print(sb);
        
    }
}
