import java.util.*;
class Main
{
    public static int gcdFind(int a,int b)
    {
        if(b == 0)
        {
            return a;
        }
        
        return gcdFind(b,a%b);
    }
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        
        int gcd=gcdFind(a,b);
        
        System.out.println("gcd :"+gcd);
        int lcm=(a*b)/gcd;
        System.out.println("lcm :"+lcm);
        
    }
}
