import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        
        int min=a<b?a:b;
        int gcd=0;
        while(min>=2)
        {
            if(a%min==0 && b%min==0)
            {
                gcd=min;
                break;
            }
            else{
                min--;
            }
            
        }
        
        System.out.println("gcd :"+gcd);
        int lcm=(a*b)/gcd;
        System.out.println("lcm :"+lcm);
        
        
        
    
        
    }
}
