 
    public static int[] Copy(int []arr,int n)
    {
        int temp[]=new int[n];
        for(int i=0;i<n;i++)
        {
            temp[i]=arr[n-i-1];
        }
        return temp;
    }
    
    
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        
        int []ans1=Copy(arr,arr.length);
        
        for(int i=0;i<ans1.length;i++)
        {
            System.out.print(ans1[i]+" ");
        }
        
        
        
    }
